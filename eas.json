{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "android": { "buildType": "apk" }
    }
  },
  "submit": {
    "production": {}
  },
  "expo": {
    "web": {
      "bundler": "metro"
    }
  }
}
